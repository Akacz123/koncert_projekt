<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="pl.koncerty.gui.UzytkownikController"
            prefHeight="400.0" prefWidth="600.0">


    <children>
        <Label layoutX="30" layoutY="20" text="Imię:"/>
        <TextField fx:id="imieField" layoutX="130" layoutY="15"/>

        <Label layoutX="30" layoutY="60" text="Nazwisko:"/>
        <TextField fx:id="nazwiskoField" layoutX="130" layoutY="55"/>

        <Label layoutX="30" layoutY="100" text="E-mail:"/>
        <TextField fx:id="emailField" layoutX="130" layoutY="95"/>

        <Label layoutX="30" layoutY="140" text="Login:"/>
        <TextField fx:id="loginField" layoutX="130" layoutY="135"/>

        <Label layoutX="30" layoutY="180" text="Hasło:"/>
        <PasswordField fx:id="hasloField" layoutX="130" layoutY="175"/>

        <Button layoutX="130" layoutY="220" text="Zarejestruj" onAction="#zarejestrujUzytkownika"/>
    </children>
</AnchorPane>
